{
  "name": "changeset-map",
  "version": "0.0.7",
  "description": "Visualize OSM changesets on a GL map (WIP)",
  "main": "js/index.js",
  "scripts": {
    "start": "npm run serve & npm run watch",
    "watch": "watchify js/render.js -o dist/bundle.js",
    "build": "browserify js/render.js | uglifyjs > dist/bundle.js",
    "serve": "http-server .",
    "lint": "eslint js/*.js",
    "lint:fix": "eslint js/*.js --fix"
  },
  "author": "Mapbox",
  "license": "ISC",
  "dependencies": {
    "@turf/bbox-polygon": "^3.13.0",
    "@turf/helpers": "^3.13.0",
    "mapbox-gl": "^0.32.1",
    "moment": "^2.11.2",
    "osm-adiff-parser-saxjs": "^1.0.0",
    "real-changesets-parser": "^1.2.0",
    "turf-buffer": "^3.0.5",
    "xhr": "^2.2.0"
  },
  "devDependencies": {
    "browserify": "^13.0.0",
    "eslint": "^3.18.0",
    "eslint-config-mourner": "^2.0.1",
    "http-server": "^0.9.0",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.7.0"
  }
